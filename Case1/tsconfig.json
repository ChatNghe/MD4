{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "../Case1/dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false
